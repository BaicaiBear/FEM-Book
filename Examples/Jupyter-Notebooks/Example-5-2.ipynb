   "source": [
    "import numpy as np\n",
    "K = np.matrix([[1,2,2], [2,4,-1], [2,-1,0]])\n",
    "e, v = np.linalg.eig(K)\n",
    "y = v * np.diag(e) * np.linalg.inv(v)"
   ]
